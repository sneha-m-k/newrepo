                        <t t-esc="item[1]"/>
                    </option>
                </select>
            </t>
            <t t-else="">
                <input id="default_value" type="text" data-type="default_value" t-att-value="widget.state.default_value"/>
            </t>
        </div>
        <t t-if="_.contains(['many2many', 'many2one', 'one2many'], widget.state.field.type)">
